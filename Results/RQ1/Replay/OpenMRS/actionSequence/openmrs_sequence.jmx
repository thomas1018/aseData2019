    	   String group = group_timestamp[1];
    	   if(group.equals(&quot;searchPerson&quot;)){
//    	   	log.info(&quot;timestamp: {}&quot;,group_timestamp[0]);
    	   	vars.put(&quot;timestamp&quot;+i,group_timestamp[0]);
    	   }
        i++;
    }
     bufRdr.close();            
}
catch (Exception ex) {
    IsSuccess = false; 
    log.error(ex.getMessage());
}
catch (Throwable thex) {
    log.error(thex.getMessage());
}

